{
  "extends": ["config:base"],
  "commitMessagePrefix": "chore:",
  "git-submodules": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchPackageNames": ["typedoc"],
      "automerge": false
    }
  ]
}
